[openstack]
auth_url=<%= @auth_url %>/v3
username=<%= @username %>
password=<%= @password %>
project_name=<%= @tenant_name %>
default_domain=<%= @default_domain %>
region=<%= @region_name %>
domain=<%= @domain %>
service_net=<%= @service_net %>
<% if @cacert -%>
keystone_cachain=<%= @cacert %>
<% end -%>

[foreman]
url=<%= @foreman_url %>
user=<%= @foreman_user %>
password=<%= @foreman_password %>

[statsd]
server=<%= @statsd_server %>
port = 8125

[state]
db=db/state.db

[mail]
smtp=<%= @smtp %>
from_addr=<%= @from_addr %>

[ldap]
server=<%= @ldap_server %>
base_dn=<%= @ldap_base_dn %>

[log]
path=/opt/himlarcli/
